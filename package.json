{
  "name": "site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:standard": "next dev",
    "dev:turbo": "cross-env TURBOPACK=1 next dev --turbo",
    "dev:turbo:trace": "cross-env TURBOPACK=1 NEXT_TURBOPACK_TRACING=1 next dev --turbo",
    "build": "cross-env NODE_ENV=production next build --no-lint",
    "start": "next start",
    "start:with-queue": "concurrently --names \"SERVER,QUEUE\" --prefix-colors \"blue,green\" \"next start\" \"node scripts/queue-monitor.js --realtime\"",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "clean": "rimraf .next out",
    "dev:clean": "npm run clean && npm run dev",
    "dev:clean:standard": "npm run clean && npm run dev:standard",
    "dev:clean:turbo": "npm run clean && npm run dev:turbo",
    "start:all": "start.bat",
    "queue-monitor": "node scripts/queue-monitor.js",
    "queue-monitor:realtime": "node scripts/queue-monitor.js --realtime",
    "queue-monitor:once": "node scripts/queue-monitor.js --once",
    "analyze": "cross-env ANALYZE=true npm run build",
    "performance-test": "node scripts/performance-test.js",
    "bundle-analyze": "node scripts/analyze-bundle.js"
  },
